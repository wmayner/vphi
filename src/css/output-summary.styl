// output-summary.styl

#output-summary-module

  .summary-items
    text-align center
    .item
      display inline-block
      margin 15px 15px

  #minimal-cut
    [class^=number]
      letter-spacing 0.5ex
      width 225px
      .cut-symbol
        padding 0 50px

  #phi-value
    font-weight bold
    [class^=number]
      width 120px
      letter-spacing .25ex
