// output-summary.styl

#output-summary-module

  .module-title
    $h = 40px
    line-height $h
    height $h

  .vphi-label
    margin-bottom 0
    margin-left 0

  .summary-items
    text-align center
    .item
      vertical-align middle
      display inline-block
      margin 15px 15px
      padding 10px 13px

  #minimal-cut
    padding 11px 13px
    .node-list
      font-size 110%
    .cut-symbol
      vertical-align middle
      font-size 160%
      margin 0 1ex

  #phi-value
    font-weight bold
